(define (problem rect-3-4)
  (:domain rectangle-world)
  (:objects n0 n1 n2 n3 n4 - int)
  (:init (xpos n0)
	 (ypos n0)
         (next n0 n1)
         (next n1 n2)
         (next n2 n3)
         (next n3 n4)
	 (safeX n3)
	 (safeY n4)
  )
  (:goal (and (xpos n3) (ypos n4)))
  (:goal-reward 1000)
  (:metric maximize (reward))
)
